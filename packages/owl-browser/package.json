{
  "name": "owl-browser",
  "version": "1.0.0",
  "description": "Custom Chromium-based browser for intelligent automation - Built by Olib AI",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "python3 scripts/build.py",
    "build:ui": "python3 scripts/build.py --ui",
    "build:http": "python3 scripts/build.py --http",
    "build:all": "python3 scripts/build.py --all",
    "build:clean": "python3 scripts/build.py --clean",
    "build:mcp": "node scripts/build-mcp-standalone.cjs",
    "build:sdk": "cd sdk && npm install && npm run build",
    "build:profiles": "python3 scripts/build_vm_profiles.py",
    "rebuild": "cd build && make owl_browser -j$(sysctl -n hw.ncpu || nproc || echo 4) && python3 ../scripts/patch_angle.py --copy-to-build",
    "rebuild:ui": "cd build && make owl_browser_ui -j$(sysctl -n hw.ncpu || nproc || echo 4) && python3 ../scripts/patch_angle.py --copy-to-build --ui",
    "rebuild:http": "cd build && make owl_http_server -j$(sysctl -n hw.ncpu || nproc || echo 4)",
    "test": "node tests/test_nla_google.cjs",
    "test:browserscan": "node tests/test_browserscan.cjs",
    "test:demographics": "node tests/test_demographics.cjs",
    "test:sdk": "node sdk/examples/simple.js",
    "build:e2e": "mkdir -p e2e-ipc-tests/build && cd e2e-ipc-tests/build && cmake .. && make -j$(sysctl -n hw.ncpu || nproc || echo 4)",
    "test:e2e": "./e2e-ipc-tests/build/ipc_test_client --browser-path ./build/Release/owl_browser.app/Contents/MacOS/owl_browser --mode full",
    "test:e2e:smoke": "./e2e-ipc-tests/build/ipc_test_client --browser-path ./build/Release/owl_browser.app/Contents/MacOS/owl_browser --mode smoke",
    "test:e2e:socket": "./e2e-ipc-tests/build/ipc_test_client --browser-path ./build/Release/owl_browser.app/Contents/MacOS/owl_browser --mode full --connection-mode socket",
    "test:e2e:pipe": "./e2e-ipc-tests/build/ipc_test_client --browser-path ./build/Release/owl_browser.app/Contents/MacOS/owl_browser --mode full --connection-mode pipe",
    "test:e2e:parallel": "./e2e-ipc-tests/build/ipc_test_client --browser-path ./build/Release/owl_browser.app/Contents/MacOS/owl_browser --mode parallel --concurrency 4",
    "test:e2e:benchmark": "./e2e-ipc-tests/build/ipc_test_client --browser-path ./build/Release/owl_browser.app/Contents/MacOS/owl_browser --mode benchmark",
    "test:e2e:stress": "./e2e-ipc-tests/build/ipc_test_client --browser-path ./build/Release/owl_browser.app/Contents/MacOS/owl_browser --mode stress --contexts 50",
    "ci:test:smoke": "node scripts/run-ci-tests.cjs smoke",
    "ci:test:core": "node scripts/run-ci-tests.cjs core",
    "ci:test:security": "node scripts/run-ci-tests.cjs security",
    "ci:test:ai": "node scripts/run-ci-tests.cjs ai",
    "ci:test:all": "node scripts/run-ci-tests.cjs all",
    "ci:test:list": "node scripts/run-ci-tests.cjs --list",
    "ci:local": "bash scripts/local-ci.sh",
    "ci:local:quick": "bash scripts/local-ci.sh quick",
    "ci:pre-commit": "pre-commit run --all-files",
    "start": "node src/mcp-server.cjs",
    "start:standalone": "node dist/mcp-server-standalone.cjs",
    "start:ui": "open 'build/Release/Owl Browser.app'",
    "start:http": "OWL_BROWSER_PATH=./build/Release/owl_browser.app/Contents/MacOS/owl_browser OWL_HTTP_TOKEN=test-token OWL_PANEL_PASSWORD=test-password OWL_IPC_TESTS_ENABLED=true OWL_IPC_TEST_CLIENT_PATH=./e2e-ipc-tests/build/ipc_test_client OWL_IPC_TEST_REPORTS_DIR=/tmp/ipc_reports ./build/Release/owl_http_server",
    "start:panel": "cd docker/frontend && npm run dev",
    "download:cef": "node scripts/download-cef.cjs && python3 scripts/patch_angle.py",
    "download:llama": "node scripts/download-llama.cjs",
    "patch:angle": "python3 scripts/patch_angle.py",
    "unpatch:angle": "python3 scripts/patch_angle.py --unpatch",
    "sdk:dev": "cd sdk && npm run dev",
    "sdk:clean": "cd sdk && npm run clean",
    "sdk:publish": "cd sdk && npm publish",
    "license:update-key": "bash scripts/update_embedded_key.sh",
    "license:check-key": "bash scripts/update_embedded_key.sh --check",
    "license:backup": "bash scripts/manage_license_keys.sh backup",
    "license:status": "bash scripts/manage_license_keys.sh status",
    "build:verify": "bash scripts/verify_build_integrity.sh"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "1.23.1",
    "node-addon-api": "^8.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "cmake-js": "^7.3.0",
    "esbuild": "0.25.0",
    "jest": "^29.0.0",
    "node-gyp": "^10.0.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
