{
  "name": "@olib-ai/owl-browser-sdk",
  "version": "2.0.0",
  "description": "Node.js SDK for Owl Browser automation - Async-first with dynamic OpenAPI method generation",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./flow": {
      "import": "./dist/flow/index.js",
      "types": "./dist/flow/index.d.ts"
    },
    "./transport": {
      "import": "./dist/transport/index.js",
      "types": "./dist/transport/index.d.ts"
    },
    "./auth": {
      "import": "./dist/auth/index.js",
      "types": "./dist/auth/index.d.ts"
    }
  },
  "files": [
    "dist",
    "openapi.json",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "prepare": "npm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "owl-browser",
    "browser-automation",
    "antidetect",
    "stealth",
    "scraping",
    "mcp",
    "ai"
  ],
  "author": "Olib AI <support@olib.ai>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Olib-AI/olib-browser.git",
    "directory": "node-sdk-v2"
  },
  "homepage": "https://www.owlbrowser.net",
  "bugs": {
    "url": "https://github.com/Olib-AI/olib-browser/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "jsonwebtoken": "^9.0.2",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.10.5",
    "@types/ws": "^8.5.13",
    "typescript": "^5.7.3",
    "vitest": "^2.1.8"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  }
}
